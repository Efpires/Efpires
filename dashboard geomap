SELECT DISTINCT
h.name,
REPLACE(hi.location_lat_desc, 'firewall', '') AS lat_name_clean,
t.value AS status,
REPLACE(h.hostname, 'firewall', '') AS host_name_clean
FROM triggers t
JOIN functions f ON t.triggerid = f.triggerid
JOIN items i ON f.itemid = i.itemid
JOIN hosts h ON i.hostid = h.hostid
JOIN hosts_groups hg ON h.groupid = hg.groupid
JOIN hstgrp g ON hg.groupid = g.groupid
JOIN host_environment hi ON h.hostid = hi.hostid
WHERE g.name LIKE 'Protege/Securanca/Firewall/%'
AND t.description ILIKE '%Base%';





=========================================================

SELECT DISTINCT
  h.name,
  hi.location_lat AS lat,
  hi.location_lon AS lon,
  t.value AS status,
  hi.notes AS Notas
FROM triggers t
INNER JOIN functions f       ON t.triggerid = f.triggerid
INNER JOIN items i           ON f.itemid = i.itemid
INNER JOIN hosts h           ON i.hostid = h.hostid
INNER JOIN hosts_groups hg   ON h.hostid = hg.hostid
INNER JOIN hstgrp g          ON hg.groupid = g.groupid
INNER JOIN host_inventory hi ON h.hostid = hi.hostid
WHERE
  g.name LIKE 'Protege/Seguranca/Firewall/%'
  AND t.description ILIKE '%Una%';



